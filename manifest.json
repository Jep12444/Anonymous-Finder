{// Required
  "manifest_version": 2,
  "name": "Anonymous",
  "version": "1.0",

  // Recommended
  "description": "A chrome extension that mutes toxic Anonymous users from the timeline",
  "icons": {"128": "anon.png"},

  // Pick one (or none)
  "browser_action": {
    "default_title":"Anonymous",
    "default_icon": "anon.png",
    "default_popup": "popup.html"

  },
  
  "optional_permissions": ["tabs"],
  
  "permissions": [
    "nativeMessaging"
  ]
}